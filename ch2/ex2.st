| input radius length area volume output |input := (UIManager default request: 'Enter the radius and length of a cylinder:') parseLiterals.radius := input at: 1.length := input at: 2.area := radius squared * Float pi.volume := area * length.output := WriteStream on: String new.output << 'The area is '.output << (area round: 4) asString.output cr.output << 'The volume is '.output << (volume round: 1) asString.UIManager default alert: output contents.